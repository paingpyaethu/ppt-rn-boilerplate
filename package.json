    "react-test-renderer": "19.2.0",
    "reactotron-react-native": "^5.1.18",
    "reactotron-react-native-mmkv": "^0.2.9",
    "reactotron-react-query": "^2.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0"
  },
  "engines": {
    "node": ">=20"
  }
}

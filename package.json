{
  "name": "RNBoilerPlate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android:developmentDebug": "cp .env.development .env && react-native run-android --mode developmentDebug --appId com.rnboilerplate.development",
    "android:stagingDebug": "cp .env.staging .env && react-native run-android --mode stagingDebug --appId com.rnboilerplate.staging",
    "android:productionDebug": "cp .env.production .env && react-native run-android --mode productionDebug",
    "ios:developmentDebug": "cp .env.development .env && react-native run-ios --scheme RNBoilerPlateDevelopment",
    "ios:stagingDebug": "cp .env.staging .env && react-native run-ios --scheme RNBoilerPlateStaging",
    "ios:productionDebug": "cp .env.production .env && react-native run-ios --scheme RNBoilerPlate",
    "pod:install": "cd ios && bundle exec pod install",
    "clean:yarn": "yarn cache clean && rm -rf node_modules && rm -rf yarn.lock && yarn install",
    "clean:ios": "cd ios && rm -rf Pods && rm -rf Podfile.lock",
    "clean:android": "rm -rf android/app/build && rm -rf android/app/.cxx && rm -rf android/.gradle",
    "clean:gradle": "cd android && ./gradlew --stop && ./gradlew clean && ./gradlew cleanBuild",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@react-navigation/bottom-tabs": "^7.10.0",
    "@react-navigation/native": "^7.1.28",
    "@react-navigation/native-stack": "^7.10.0",
    "@tanstack/react-query": "^5.90.19",
    "freerasp-react-native": "^4.3.2",
    "i18next": "^25.7.4",
    "intl-pluralrules": "^2.0.1",
    "ky": "^1.8.2",
    "react": "19.2.0",
    "react-hook-form": "^7.71.1",
    "react-i18next": "^16.5.3",
    "react-native": "0.83.1",
    "react-native-config": "^1.6.1",
    "react-native-edge-to-edge": "^1.7.0",
    "react-native-keyboard-controller": "^1.20.7",
    "react-native-mmkv": "^4.1.1",
    "react-native-nitro-modules": "^0.33.2",
    "react-native-reanimated": "^4.2.1",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "4.19.0",
    "react-native-svg": "^15.15.1",
    "react-native-worklets": "^0.7.2",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-transform-export-namespace-from": "^7.27.1",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@eslint/js": "^9.23.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.83.1",
    "@react-native/eslint-config": "0.83.1",
    "@react-native/metro-config": "0.83.1",
    "@react-native/typescript-config": "0.83.1",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@testing-library/react-native": "^13.2.2",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.13.17",
    "@types/react": "^19.2.0",
    "@types/react-test-renderer": "^19.1.0",
    "@typescript-eslint/parser": "^8.29.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
